<%- include("partials/header") %> 
<div class="entry">
    <%- include("navbar") %> 
    <img src="https://cdn.pixabay.com/photo/2016/08/08/09/17/avatar-1577909_1280.png" alt="Person" class="circular-image contact-entry" id="profile-picture" />
    <div class="container profile text-center">
        <div class="row">
            <div class="col-sm-12">
                <input type="text" name="full-name" id="full-name" placeholder='Name' class='profile-full-row' />
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <input type="text" name="company-or-school" id="company-or-school" placeholder='Company/School' class='profile-half-row' />
                <input type="text" name="group" id="group" placeholder='Group' class='profile-half-row' />
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <input type="tel" name="phone" id="phone" placeholder='Phone' class='profile-half-row' pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}" />
                <input type="email" name="email" id="email" placeholder='Email' class='profile-half-row' />
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <input type="text" name="full-address" id="full-address" placeholder='Address' class='profile-full-row' />
            </div>
        </div>
    </div>
    <br></br>
    <br></br>
    <br></br>
    
    <a href="/book" class="discard">Discard</a>
    <a href="/book" class="save">Save</a>
</div>
<%- include("partials/footer") %> 
